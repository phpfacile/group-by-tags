{
    "name": "phpfacile/group-by-tags",
    "description": "Dummy implementation of services to manage items (of any kind) that can be grouped by their tag values.",
    "keywords": ["group", "interface", "tags"],
    "homepage": "https://github.com/phpfacile/group-by-tags",
    "license": "MIT",
    "authors": [
        {
            "name": "PHP Facile!",
            "homepage": "https://www.phpfacile.com/"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "phpfacile/group": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "squizlabs/php_codesniffer": "3.*",
        "zendframework/zend-log": "^2.10.0"
    },
    "scripts": {
        "cs-check": "phpcs -s src",
        "cs-check-psr": "phpcs -s --standard=PSR1,PSR2 src",
        "cs-check-pear": "phpcs -s --standard=PEAR src",
        "cs-fix": "phpcbf src",
        "test": "phpunit --colors=always --testsuite \"all tests\""
    },
    "autoload": {
        "psr-4": {
          "PHPFacile\\Group\\ByTags\\": "src/"
        }
    }
}
